    ServerAdmin mtrognon@commonhelp.it
    ServerName devserver.commonhelp.it
    DocumentRoot /var/www/template
    <Directory /var/www/template>
            AllowOverride None
            Require all granted
            Allow from All
    </Directory>
</VirtualHost>
